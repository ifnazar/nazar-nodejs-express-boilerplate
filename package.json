    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-promise": "^4.1.1",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "nodemon": "^1.19.1",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
